	column runnigTotal = ```
			
			                    CALCULATE(SUM(Purchases[Purchase]),
			                        FILTER(ALLSELECTED(Purchases),
			                            Purchases[Article No] = EARLIEST(Purchases[Article No]) &&
			                            Purchases[Code] = EARLIEST(Purchases[Code]) &&
			                            Purchases[Date] >= EARLIEST(Purchases[Date])))
			```
		formatString: 0
		lineageTag: 2bdfd13e-2935-4bea-8429-aba70119d04f
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column closing = ```
			CALCULATE(SUM(OBS_sheet[CBS Qty]), 
			                                FILTER(OBS_sheet,
			                                OBS_sheet[Location Id] = EARLIEST(Purchases[Code])
			                                &&  OBS_sheet[Article No] = EARLIEST(Purchases[Article No])))
			```
		formatString: 0
		lineageTag: 599a0b82-f442-49a7-a8d7-e38ecb0cbfb2
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column diff = ```
			
			    [Closing] - Purchases[runnigTotal]
			```
		formatString: 0
		lineageTag: fb682974-f79a-42da-99b1-f46fe419d7a1
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column islatestdate = ```
			
			
			var _latestpoint = 
			                    CALCULATE(MIN(Purchases[diff]),
			                        FILTER(ALL(Purchases),
			                            Purchases[Article No] = EARLIEST(Purchases[Article No]) &&
			                            Purchases[Code] = EARLIEST(Purchases[Code]) &&
			                            Purchases[diff] > EARLIEST(Purchases[diff])))
			
			RETURN
			    SWITCH(TRUE(),
			    [Closing] = 0, FALSE(),
			     _latestpoint >= 1, TRUE(),
			      ISBLANK(_latestpoint) , TRUE())
			    
			```
		formatString: """TRUE"";""TRUE"";""FALSE"""
